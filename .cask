# Install cask apps
function installcask() {
	brew cask install "${@}" 2> /dev/null
}

# Browsers
installcask google-chrome
installcask firefox

# Cloud
installcask dropbox
installcask google-drive

# Others
installcask skype
installcask evernote
installcask android-file-transfer
installcask colorpicker
installcask slack
installcask teamviewer
installcask spotify

# Code
installcask arduino
installcask android-studio
installcask wireshark
installcask virtualbox
installcask java
installcask docker
installcask vagrant
installcask psequel
installcask atom
